<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.2//EN" "https://netbeans.apache.org/dtds/EditorFontsColors-1_2.dtd">
<fontscolors>
    <fontcolor default="number" foreColor="magenta" name="latte-markup-number"/>
    <fontcolor default="identifier" name="latte-markup-symbol"/>
    <fontcolor default="error" name="latte-error"/>
    <fontcolor foreColor="ffbd93f9" name="latte-delimiter">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="identifier" name="latte-markup-cast"/>
    <fontcolor foreColor="ff8be9fd" name="latte-markup-variable"/>
    <fontcolor default="string" foreColor="yellow" name="latte-markup-string"/>
    <fontcolor default="keyword" name="latte-markup-keyword"/>
    <fontcolor default="comment" name="latte-comment"/>
    <fontcolor default="identifier" foreColor="ffff79c6" name="latte-markup-macro">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="separator" foreColor="ffff79c6" name="latte-markup-char"/>
    <fontcolor default="whitespace" name="latte-markup-whitespace"/>
</fontscolors>
