<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.2//EN" "https://netbeans.apache.org/dtds/EditorFontsColors-1_2.dtd">
<fontscolors>
    <fontcolor default="error" name="twig_other"/>
    <fontcolor name="twig_block_delimiter">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="identifier" name="twig_name"/>
    <fontcolor name="twig_interpolation"/>
    <fontcolor default="keyword" foreColor="ff00a0a0" name="twig_punctuation"/>
    <fontcolor name="twig_var_delimiter">
        <font style="bold"/>
    </fontcolor>
    <fontcolor default="comment" name="twig_comment"/>
    <fontcolor default="string" foreColor="yellow" name="twig_string"/>
    <fontcolor default="keyword" foreColor="ff00a0a0" name="twig_operator"/>
    <fontcolor default="number" name="twig_number"/>
    <fontcolor default="keyword" foreColor="ff00a0a0" name="twig_tag"/>
</fontscolors>
